generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UrlTicket {
  id      Int @unique @default(autoincrement()) @id
  start   Int
  end     Int
  current Int
}

model Url {
  id        Int      @default(autoincrement()) @id
  short_url String   @unique
  long_url  String
  createdAt DateTime @default(now())
}
